#include<bits/stdc++.h>
using namespace std;

// Problem:Find the length of linked list
// Platform:Take u Forward

class Solution {
public:
    int getLength(ListNode* head) {
        ListNode*temp=head;
        int count=0;
        while(temp!=nullptr){
            count++;
            temp=temp->next;
        }
        return count;
    }
};
